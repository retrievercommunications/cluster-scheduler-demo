{
    "containerDefinitions": [
      {
        "name": "hello-server",
        "image": "retriever/hello-server:1.0",
        "memory": 128,
        "portMappings": [
            {
                "hostPort": 8080,
                "containerPort": 8080,
                "protocol": "tcp"
            },
            {
                "hostPort": 8081,
                "containerPort": 8081,
                "protocol": "tcp"
            }
        ],
        "essential": true
      }
    ],
    "family": "hello-server-family"
}